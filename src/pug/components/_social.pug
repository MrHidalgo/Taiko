// SOCIAL :: START
-
  var socialArr = [
    'instagram',
    'youtube',
    'facebook',
    'vkontakte'
  ];

.social
  -for(var i = 0, len = socialArr.length; i < len; i++)
    a(
      href='#'
      target='_blank'
      class='social__name'
    )!= socialArr[i]

    if (i !== len - 1)
      span.social__line!= '&nbsp;&nbsp;&nbsp;|&nbsp;&nbsp;&nbsp;'

// SOCIAL :: END
